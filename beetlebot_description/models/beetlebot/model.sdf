                    <pose>0 0 0 0 0 0</pose>
                    <geometry>
                        <cylinder>
                            <radius>0.15</radius>
                            <length>0.07</length>
                        </cylinder>
                    </geometry>
                    <material>
                        <ambient>0.6375968739867731 0.04373502925049377 0.008023192982520563 1</ambient>
                        <diffuse>0.6375968739867731 0.04373502925049377 0.008023192982520563 1</diffuse>
